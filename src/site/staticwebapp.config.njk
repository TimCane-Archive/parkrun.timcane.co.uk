---
permalink: staticwebapp.config.json
layout: api
---
{
    "routes": [
        {
        "route": "/latest",
        "redirect": "/dates/{{ parkrun.root.LastRun | date('yyyy') }}/{{ parkrun.root.LastRun | date('mm') }}/{{ parkrun.root.LastRun | date('dd') }}/"
        }
    ],
    "responseOverrides": {
        "400": {
            "rewrite": "/bad-request/"
        },
        "401": {
            "rewrite": "/unauthorized/"
        },
        "403": {
            "rewrite": "/forbidden/"
        },
        "404": {
            "rewrite": "/page-not-found/"
        }
    }
}